{
  "name": "foundation",
  "version": "0.1.0",
    "dependencies": [
      {
        "name": "daxa",
        "features": [
          "utils-pipeline-manager-glslang",
          "utils-pipeline-manager-slang",
          "utils-task-graph",
          "utils-imgui"
        ]
      },
      {
        "name": "glfw3",
        "features": [
          "xlib",
          "wayland"
        ]
      },
      {
        "name": "imgui",
        "features": [
            "glfw-binding",
            "docking-experimental"
          ]
      },
      "glm",
      "fastgltf",
      "stb",
      "zstd",
      "flecs",
      "tracy",
      "imguizmo",
      "sol2",
      "metis",
      "meshoptimizer",
      "lua"
    ],
  "builtin-baseline": "f5c5b8ddbb88dec7eed4a5ac54c36d1c64d2d29d",
  "vcpkg-configuration": {
    "overlay-ports": [
      "deps/Daxa",
      "deps/Daxa/cmake/vcpkg-overlay-ports/glfw3",
      "deps/Daxa/cmake/vcpkg-overlay-ports/fsr2",
      "deps/daxa/cmake/vcpkg-overlay-ports/shader-slang",
      "cmake/vcpkg-overlay-ports/meshoptimizer",
      "cmake/vcpkg-overlay-ports/sol2",
      "cmake/vcpkg-overlay-ports/fastgltf",
      "cmake/vcpkg-overlay-ports/imgui",
      "cmake/vcpkg-overlay-ports/imguizmo"
    ]
  }
}
